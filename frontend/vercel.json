{
  "version": 2,
  "routes": [
    {
      "source": "/nft/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(WhatsApp|facebookexternalhit|twitterbot|LinkedInBot)"
        }
      ],
      "headers": {
        "Cache-Control": "s-maxage=3600"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
